{
  "name": "elba",
  "version": "3.0.0",
  "description": "Discord music bot built with discord.js (Modified by OwenBellowen)",
  "main": "index.ts",
  "author": "Owen <sheanowen3@gmail.com>",
  "private": true,
  "homepage": "https://github.com/OwenPotent/elba-bot",
  "repository": "github:OwenPotent/elba-bot",
  "bugs": "https://github.com/OwenPotent/elba-bot/issues",
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "dev": "nodemon index.ts",
    "start": "bun index.ts",
    "build": "tsc --build",
    "prod": "npm run build && node ./dist/index.js",
    "commit": "cz",
    "format": "npx prettier --write '**/*.ts' --config .prettierrc"
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.1",
    "array-move": "^3.0.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "ffmpeg-static": "^4.4.1",
    "i18n": "^0.15.1",
    "lyrics-finder": "^21.0.5",
    "play-dl": "^1.9.7",
    "soundcloud-downloader": "^0.2.3",
    "string-progressbar": "^1.0.4",
    "youtube-sr": "~4.3.0"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.12",
    "@types/node": "^20.14.0",
    "cz-conventional-changelog": "^3.3.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.9.0",
    "libsodium-wrappers": "^0.7.13",
    "opusscript": "^0.1.1",
    "sodium-native": "^4.1.1",
    "tweetnacl": "^1.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "disableScopeLowerCase": false,
      "disableSubjectLowerCase": false
    }
  }
}
